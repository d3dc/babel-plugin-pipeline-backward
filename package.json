{
  "name": "babel-plugin-pipeline-backward",
  "version": "0.0.1",
  "description": "Babel plugin to provide F# pipe backward behavior to match the proposed pipeline operator",
  "homepage": "https://github.com/d3dc/babel-plugin-pipeline-backward",
  "repository": {
    "type": "git",
    "url": "https://github.com/d3dc/babel-plugin-pipeline-backward.git"
  },
  "bugs": {
    "url": "https://github.com/d3dc/babel-plugin-pipeline-backward/issues"
  },
  "license": "MIT",
  "keywords": [
    "babel",
    "babel-plugin",
    "pipe",
    "pipeline"
    "pipeline operator",
    "pipe backward",
    "pipeline backward",
  ],
  "author": {
    "name": "Sam Richard",
    "username": "d3dc"
  },
  "main": "build/index.js",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-implicit-return": "^1.0.0",
    "babel-plugin-strict-equality": "^1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^2.13.0",
    "mocha": "^2.4.5",
    "snyk": "^1.14.3"
  },
  "scripts": {
    "prebuild": "npm run build:clean && npm run test",
    "build": "npm run build:js",
    "build:clean": "rm -rf ./build",
    "build:js": "./node_modules/.bin/babel src -d build --source-maps=inline",
    "snyk": "./node_modules/.bin/snyk",
    "snyk:auth": "npm run -s snyk -- auth $SNYK_TOKEN",
    "snyk:monitor": "npm run snyk -- monitor",
    "snyk:test": "npm run snyk -- test",
    "snyk:wizard": "npm run snyk -- wizard",
    "pretest": "npm run snyk:test && npm run lint",
    "test": "npm run test:js",
    "test:cov": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:js": "npm run test:js:cov",
    "test:js:cov": "NODE_ENV=test ./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha",
    "test:js:nocov": "NODE_ENV=test ./node_modules/.bin/babel-node ./node_modules/.bin/_mocha",
    "watch": "npm run watch:js",
    "watch:js": "npm run build:js -- --watch"
  }
}
